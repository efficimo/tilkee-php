{
    "name": "efficimo/tilkee-php",
    "description": "PHP implementation of Tilkee API https://tilkee.readme.io/ (fork https://gitlab.com/hbs-research/tilkee-php) ",
    "type": "library",
    "license": "MIT",
    "require": {
        "jane-php/open-api-runtime": "^5.1",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/multipart-stream-builder": "^1.0"
    },
    "authors": [
        {
            "name": "RaphaÃ«l Davaillaud",
            "email": "raphael.davaillaud.hbs+git@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "HbsResearch\\Tilkee\\": "src",
            "HbsResearch\\Tilkee\\API\\": "generated"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HbsResearch\\Tilkee\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "7.2"
        }
    },
    "require-dev": {
        "jane-php/open-api": "^5.1",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.6",
        "php-http/curl-client": "^2.0",
        "php-http/message": "^1.8",
        "http-interop/http-factory-guzzle": "^1.0",
        "phpunit/phpunit": "^7"
    },
    "archive": {
        "exclude": ["/bin",
            "Dockerfile"
        ]
    }
}
