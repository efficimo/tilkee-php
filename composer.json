{
    "name": "hbs-research/tilkee-php",
    "description": "PHP implementation of Tilkee API https://tilkee.readme.io/",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "7.1.* || 7.2.*",
        "php-http/guzzle6-adapter": "^1.1",
        "guzzlehttp/psr7": "^1.4",
        "php-http/message": "^1.6",
        "php-http/multipart-stream-builder": "^1.0"
    },
    "authors": [
        {
            "name": "RaphaÃ«l Davaillaud",
            "email": "raphael.davaillaud.hbs+git@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tilkee\\": "src/",
            "Tilkee\\API\\": "generated/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "7.1"
        }
    },
    "require-dev": {
        "jane/open-api": "^3.1"
    },
    "archive": {
        "exclude": ["/bin", "Dockerfile"]
    }
}
